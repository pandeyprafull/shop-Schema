{
    "name": "@shop_org/schemas",
    "version": "1.0.5",
    "description": "Interfaces, entities, enums and etc for E-shop",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Prafull Pandey <prafullpandey68@gmail.com>",
    "private": false,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint . --ext .ts",
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:generate": "npm run build && ts-node ./node_modules/.bin/typeorm migration:generate -n change-migration-name-here",
        "migration:run": "ts-node ./node_modules/.bin/typeorm -c=development migration:run",
        "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
        "prepare": "npm run build",
        "postpublish": "npm run clean"
    },
    "keywords": [],
    "license": "ISC",
    "files": [
        "dist/**/*"
    ],
    "devDependencies": {
        "@types/node": "^14.0.27",
        "@types/randomstring": "^1.1.7",
        "@typescript-eslint/parser": "^3.8.0",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "eslint": "^7.32.0",
        "pg": "^7.18.1",
        "randomstring": "^1.2.1",
        "rxjs": "^6.6.2",
        "typeorm": "^0.2.37",
        "uuid": "^8.3.2"
    }
}